
source ~/.zshrc.common

test -e "${HOME}/.iterm2_shell_integration.zsh" && source "${HOME}/.iterm2_shell_integration.zsh"

export PATH=/usr/local/opt/ruby/bin:$PATH
export PATH=$HOME/.local/bin:$PATH
export PATH=/Users/ywchoi/Library/Python/3.9/bin:${PATH}

alias vim=nvim
alias vi=vim
alias cdw="cd ${HOME}/Workspace"
cd-to-workspace() {
  zle push-line # Clear buffer. Auto-restored on next prompt.
  BUFFER="cd ${HOME}/Workspace"
  zle accept-line
  local ret=$?
  unset dir # ensure this doesn't end up appearing in prompt expansion
  zle reset-prompt
  return $ret
}
zle     -N   cd-to-workspace
bindkey '^W' cd-to-workspace

alias readlink=greadlink

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh

alias julia=/Applications/Julia-1.6.app/Contents/Resources/julia/bin/julia

# export PYTHONPATH=/usr/local/lib/python3.9/site-packages:${PYTHONPATH}
source /opt/intel/oneapi/setvars.sh >& /dev/null

conda activate local
