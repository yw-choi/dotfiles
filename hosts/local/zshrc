source ~/.zshrc.common

test -e "${HOME}/.iterm2_shell_integration.zsh" && source "${HOME}/.iterm2_shell_integration.zsh"

export PATH=$HOME/.local/bin:$PATH
export PATH=/usr/local/opt/ruby@3.1/bin:${PATH}

export EDITOR=nvim
alias vim=nvim
alias vi=nvim

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh

# source /opt/intel/oneapi/setvars.sh 

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/ywchoi/miniconda3/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/ywchoi/miniconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/ywchoi/miniconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/ywchoi/miniconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

conda activate local
